{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint .",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "e2e:build-ios-debug": "detox build -c ios.sim.debug",
    "e2e:build-ios-release": "detox build -c ios.sim.release",
    "e2e:test-ios-debug": "detox test -c ios.sim.debug",
    "e2e:test-ios-release": "detox test -c ios.sim.release",
    "e2e:build-android-debug": "yarn android:clean && detox build -c android.emu.debug",
    "e2e:build-android-release": "yarn android:clean && detox build -c android.emu.release",
    "e2e:test-android-debug": "detox test -c android.emu.debug",
    "e2e:test-android-release": "detox test -c android.emu.release",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --coverage",
    "e2e:build-att-debug": "yarn android:clean && detox build -c android.att.debug",
    "e2e:test-att-debug": "detox test -c android.att.debug",
    "test:coverage-all": "yarn test:coverage-check-strict && yarn test:coverage-report && node scripts/coverage-warning.js",
    "test:coverage-check-strict": "jest --coverage --coverageThreshold='{ \"global\": { \"functions\": 80, \"statements\": 80 } }'",
    "test:coverage-report": "jest --coverage",
    "prepare": "husky install",
    "android:build:dev": "cd android && ./gradlew assembleRelease",
    "run:android:dev": "npx react-native run-android",
    "sentry:upload": "node scripts/sentry-upload.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@notifee/react-native": "^9.1.8",
    "@react-native-firebase/analytics": "22.2.1",
    "@react-native-firebase/app": "22.2.1",
    "@react-native-firebase/messaging": "^22.2.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/react-native": "^6.16.0",
    "@testing-library/react-native": "^13.2.0",
    "husky": "^9.1.7",
    "react": "18.3.1",
    "react-hook-form": "^7.57.0",
    "react-native": "0.75.2",
    "react-native-config": "^1.5.5",
    "react-native-device-info": "^14.0.4",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-notifier": "^2.0.0",
    "react-native-onesignal": "^5.2.12",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.11.1",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "telnet-client": "^2.2.5",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.2",
    "@react-native/eslint-config": "0.75.2",
    "@react-native/metro-config": "0.75.2",
    "@react-native/typescript-config": "0.75.2",
    "@testing-library/jest-native": "^5.4.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "detox": "^20.39.0",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.4.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
